import java.util.Scanner;

public class Test
{
    public static void main(String[] args) {
        
    	int income = 4001;
    	if (income > 3000) {
    		  System.out.println("Income is greater than 3000");
    		}
    		else if (income > 4000) {
    		  System.out.println("Income is greater than 4000");
    		}
}
}